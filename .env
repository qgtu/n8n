# ============================================
# n8n Configuration File - Local Development
# ============================================

# ------------------------------
# Server Settings
# ------------------------------
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Base URL và Webhook
N8N_EDITOR_BASE_URL=http://localhost:5678
WEBHOOK_URL=http://localhost:5678/

# ------------------------------
# Database (SQLite - Mặc định)
# ------------------------------
# n8n sẽ tự động tạo file database.sqlite
# DB_TYPE=sqlite
# DB_SQLITE_DATABASE=database.sqlite

# PostgreSQL (Nếu cần dùng)
# DB_TYPE=postgresdb
# DB_POSTGRESDB_HOST=localhost
# DB_POSTGRESDB_PORT=5432
# DB_POSTGRESDB_DATABASE=n8n
# DB_POSTGRESDB_USER=postgres
# DB_POSTGRESDB_PASSWORD=postgres

# ------------------------------
# Timezone & Locale
# ------------------------------
GENERIC_TIMEZONE=Asia/Ho_Chi_Minh
DEFAULT_LOCALE=vi

# ------------------------------
# Execution Settings
# ------------------------------
EXECUTIONS_MODE=regular
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Workflow Timeout (milliseconds) - mặc định không giới hạn
# EXECUTIONS_TIMEOUT=-1
# EXECUTIONS_TIMEOUT_MAX=3600000

# ------------------------------
# Security
# ------------------------------
# Basic Authentication (tắt cho local dev)
N8N_BASIC_AUTH_ACTIVE=false


# Encryption Key (tự động tạo nếu không set)
# N8N_ENCRYPTION_KEY=

# ------------------------------
# Logging
# ------------------------------
N8N_LOG_LEVEL=debug
N8N_LOG_OUTPUT=console

# ------------------------------
# Community Nodes
# ------------------------------
# Cho phép cài đặt community nodes
N8N_COMMUNITY_PACKAGES_ENABLED=true

# ------------------------------
# Performance
# ------------------------------
# Số worker processes (mặc định 1)
# EXECUTIONS_PROCESS=main

# ------------------------------
# Development
# ------------------------------
# Tắt telemetry cho local dev
N8N_DIAGNOSTICS_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=true

